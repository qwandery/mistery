@font-face {
  font-family: 'pt_sansregular';
  src: url(<%= asset_path 'PTS55F-webfont.eot' %>);
  src: url(<%= asset_path 'PTS55F-webfont.eot?#iefix' %>) format('embedded-opentype'),
       url(<%= asset_path 'PTS55F-webfont.woff' %>) format('woff'),
       url(<%= asset_path 'PTS55F-webfont.ttf' %>) format('truetype'),
       url(<%= asset_path 'PTS55F-webfont.svg#pt_sansregular' %>) format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'LatoLatin';
  src: url(<%= asset_path 'LatoLatin-Regular.eot' %>);
  src: url(<%= asset_path 'LatoLatin-Regular.eot?#iefix' %>) format('embedded-opentype'),
       url(<%= asset_path 'LatoLatin-Regular.woff' %>) format('woff'),
       url(<%= asset_path 'LatoLatin-Regular.ttf' %>) format('truetype');
  font-style: normal;
  font-weight: normal;
  text-rendering: optimizeLegibility;
}

@font-face {
  font-family: 'OpenSans';
  src: url(<%= asset_path 'OpenSans-Regular.ttf' %>);
  font-style: normal;
  font-weight: normal;
  text-rendering: optimizeLegibility;
}
